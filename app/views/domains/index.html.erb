Server time is <%= Time.now %>

<h1>Listing domains</h1>

<table>
  <tr>
    <th>Domain</th>
    <th>Email</th>
    <th>Confirm code</th>
    <th>Confirmed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @domains.each do |domain| %>
  <tr>
    <td><%= domain.domain %></td>
    <td><%= domain.email %></td>
    <td><%= domain.confirm_code %></td>
    <td><%= domain.confirmed %></td>
    <td><%= link_to 'Show', domain %></td>
    <td><%= link_to 'Edit', edit_domain_path(domain) %></td>
    <td><%= link_to 'Destroy', domain, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Domain', new_domain_path %>
